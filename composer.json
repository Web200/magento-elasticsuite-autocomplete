{
    "name": "web200/magento-elasticsuite-autocomplete",
    "description": "Magento 2 Module to speed autocomplete search",
    "require": {
        "php": "^7.0|^8.0"
    },
    "type": "magento2-component",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "replace": {
        "web200/module-elasticsuite-autocomplete": "self.version"
    },
    "autoload": {
        "files": [
            "src/module-elasticsuite-autocomplete/registration.php"
        ],
        "psr-4": {
            "Web200\\ElasticsuiteAutocomplete\\": "src/module-elasticsuite-autocomplete"
        }
    },
    "extra": {
        "map": [
            [
                "src/pub/search.php",
                "pub/search.php"
            ]
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
